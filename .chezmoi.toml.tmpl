{{- $myubuntu := promptBool "myubuntu" -}}
{{- $myfedora := promptBool "myfedora" -}}
{{- $usezsh := promptBool "usezsh" -}}
{{- $hasslurm := promptBool "hasslurm" -}}
encryption = "gpg"
[gpg]
    symmetric = true
[data]
    myubuntu = {{ $myubuntu }}
    myfedora = {{ $myfedora }}
    usezsh = {{ $usezsh }}
    hasslurm = {{ $hasslurm }}
    csd3 = false
    powerlinehostname = "login"
